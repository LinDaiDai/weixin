/**app.wxss**/
@import "./assets/font/font.wxss";
page{
  height: 100%;
}
.header{
  height:100rpx;
  width:100%;
  background:#00BFFF;
  display:flex;
  flex-direction:row;
  align-items:center;
  position: fixed;
  top: 0;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
.content{
  position: fixed;
  left: 0px;
  width: 100%;
  height: 100%;
  overflow: scroll;
}
.footer{
    position: fixed;
    bottom: 0;
    height: 110rpx;
    width: 100%;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.clear::after{
  display: block;
  content: "";
  clear: both;
}
.font_30{
  font-size: 30rpx;
}
.font_40{
  font-size:40rpx;
}
.font_50{
  font-size: 50rpx;
}
.font_60{
  font-size: 60rpx;
}
.font_w_700{
  font-weight: 700;
}
.text_left{
  text-align: left;
}
.text_right{
  text-align: right;
}
.pos_rel{
  position: relative;
}
.pos_abs{
  position: absolute;
}