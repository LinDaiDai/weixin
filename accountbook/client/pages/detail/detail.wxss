.container{
  position: relative;
  height: 100%;
}
.header{
    height: 220rpx;
}
.content{
  top: 220rpx;
}
.typeList{
  height:100%;
  padding:20rpx;
}
.typeItem{
    width: 100%;
    height: 100rpx;
}
.headerType{
    height: 100%;
    width: 140rpx;
    margin:0 auto;
    text-align:center;
}
.pic{
    height: 140rpx;
    width: 140rpx;
    line-height:140rpx;
    background-color:#fff;
    border-radius:50%;
    transition:all 0.3s;
    color: #00BFFF;
}
.name{
    height:60rpx;
    font-size:27rpx;
    padding-top:15rpx;
    color: #fff;
}